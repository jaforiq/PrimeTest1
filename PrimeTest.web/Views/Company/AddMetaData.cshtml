@model PrimeTechTest.DTOs.AddColumnMetaDataDto


<h1>Add ColumnMetaData</h1>

<form method="post">
    <div class="mt-3">
        <label class="form-label">CompanyUniqueId</label>
        <input type="text" class="form-control" asp-for="CompanyUniqueId"/>
    </div>

    <div class="mt-3">
        <label class="form-label">ColumnInfo</label>
        <input type="text" class="form-control" asp-for="ColumnInfo"/>
    </div>

    <div class="mt-3">
        <label class="form-label">CompanyPrimaryId</label>
        <input type="text" class="form-control" asp-for="CompanyId"/>
    </div>

    <div class="mt-3">
        <button type="submit" class="btn btn-primary">Save</button>
    </div>

</form>
