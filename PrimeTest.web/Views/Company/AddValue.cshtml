@{
}

@model PrimeTechTest.DTOs.AddColumnValueDto

<h1>Add Column Value</h1>

<form method="post">
    <div class="mt-3">
        <label class="form-label">CompanyUniqueId</label>
        <input type="text" class="form-control" asp-for="CompanyUniqueId" />
    </div>

    <div class="mt-3">
        <label class="form-label">Value</label>
        <input type="text" class="form-control" asp-for="Value" />
    </div>

    <div class="mt-3">
        <label class="form-label">PrimaryId</label>
        <input type="text" class="form-control" asp-for="CompanyId" />
    </div>

    <div class="mt-3">
        <button type="submit" class="btn btn-primary">Save</button>
    </div>

</form>