@{
}

@model PrimeTechTest.DTOs.AddCompanyInfoDto

<h1>Add Company</h1>

<form method="post">
<div class="mt-3">
    <label class="form-label">CompanyUniqueId</label>
    <input type="text" class="form-control" asp-for="CompanyUniqueId"/>
</div>

<div class="mt-3">
    <label class="form-label">Name</label>
    <input type="text" class="form-control" asp-for="Name"/>
</div>

<div class="mt-3">
    <label class="form-label">Address</label>
    <input type="text" class="form-control" asp-for="Address"/>
</div>

<div class="mt-3">
    <button type="submit" class="btn btn-primary">Save</button>
</div>

</form>